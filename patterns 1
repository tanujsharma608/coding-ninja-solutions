#Code : Square Patter

solutons

n = int(input())
k = 1
while k<=n:
    j = 1
    while j<=n:
        j = j + 1
        print(n, end='')
    print()
    k = k + 1
  *************************************************************************************************************************************************************8
  
  #2  Triangular Star Pattern

n = int(input())
i = 1
while i<=n:
    j = 1
    while j<=i:
        j = j + 1
        print("*", end='')
    print()


    i = i + 1
********************************************************************************************************************************************************************8
#3 Code : Triangle Number Pattern
n=int(input())
i=1
while i<=n:
    j=1
    while j<=i:
        j=j+1
        print(i,end="")
    print()
    i=i+1
 *****************************************************************************************************************************************************************
 #4 
 Code : Reverse Number Pattern
 n = int(input())
for i in range(n):
    for j in range(i,-1,-1):
        print(j+1,end="")
    print()
*******************************************************************************************************************************************************************
#5
#5 Character Pattern

n = int(input())
i = 1
while i<=n:
    j = 1
    while j<=i:
        charp = chr(ord('A') + i+j-1 - 1)
        print(charp, end = '')
        j +=1
    print()
    i = i + 1
  ****************************************************************************************************************************************************************
 #6 Interesting Alphabets


n = int(input())
for i in range(n, 0 ,-1):
    for j in range(i, n+1,1): 
        print(chr(ord('A')+ j -1 ), end = '')
    print()
